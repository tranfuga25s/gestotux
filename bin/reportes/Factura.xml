 <topmargin>25</topmargin>
 <bottommargin>25</bottommargin>
 <rightmargin>25</rightmargin>
 <leftmargin>25</leftmargin>
 <querysource>
  <name>factura</name>
  <sql>SELECT strftime("%d/%m/%Y", f.fecha) AS fecha,
       f.total As total,
       substr( '0000000000' || f.serie, -5, 5 ) || '-' || substr( '0000000000' || f.numero, -5, 5 ) AS NumSerie
FROM factura AS f, clientes AS c
WHERE f.id_cliente = c.id AND f.id_factura = &lt;? value( "id_factura" ) ?>;</sql>
 </querysource>
 <querysource>
  <name>items</name>
  <sql>SELECT cantidad,
       texto,
       precio_unitario,
       cantidad*precio_unitario AS subtotal,
       id_factura
FROM item_factura
WHERE id_factura = &lt;? value( "id_factura" ) ?>
ORDER BY id_item_factura;</sql>
 </querysource>
