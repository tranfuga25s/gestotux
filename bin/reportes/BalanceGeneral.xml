 <querysource>
  <name>compras_mes</name>
  <sql>SELECT strftime( "%m/%Y", c.fecha ) AS mes,
       SUM( c.total ) AS total
FROM compras AS c
GROUP BY strftime( "%m/%Y", c.fecha )</sql>
 </querysource>
 <querysource>
  <name>compras_total</name>
  <sql>SELECT SUM( c.total ) AS total
FROM compras AS c</sql>
 </querysource>
 <querysource>
  <name>presupuesto_mes</name>
  <sql>SELECT strftime( "%m/%Y", p.fecha ) AS mes,
             SUM( p.total ) AS total
FROM presupuestos AS p
GROUP BY 1</sql>
 </querysource>
 <querysource>
  <name>presupuesto_total</name>
  <sql>SELECT SUM( p.total ) AS total
FROM presupuestos AS p</sql>
 </querysource>
 <querysource>
  <name>ventas_mes</name>
  <sql>SELECT strftime( "%m/%Y", f.fecha ) AS mes,
             SUM( f.total ) AS total
FROM factura AS f
GROUP BY 1</sql>
 </querysource>
 <querysource>
  <name>ventas_total</name>
  <sql>SELECT SUM( f.total ) AS total
FROM factura AS f</sql>
 </querysource>
 <querysource>
  <name>gastos_mes</name>
  <sql>SELECT strftime( "%m/%Y", g.fecha ) AS mes,
             cg.nombre AS categoria,
             SUM(g.costo ) AS total
FROM gastos AS g, categoria_gastos as cg
WHERE g.id_categoria = g.id_categoria
GROUP BY 1</sql>
 </querysource>
 <querysource>
  <name>gastos_total</name>
  <sql>SELECT SUM( costo ) AS total
FROM gastos</sql>
 </querysource>
 <querysource>
  <name>recibos_mes</name>
  <sql>SELECT strftime( "%m/%Y", r.fecha_pago ) as mes,
             SUM( r.precio ) as total
FROM recibos AS r
GROUP BY strftime( "%m/%Y", r.fecha_pago )
ORDER BY strftime( "%Y", r.fecha_pago ), strftime( "%m", r.fecha_pago )</sql>
 </querysource>
 <querysource>
  <name>recibos_total</name>
  <sql>SELECT SUM( r.precio ) as total
FROM recibos AS r</sql>
 </querysource>
 <querysource>
  <name>caja_item</name>
  <sql>SELECT nombre, saldo
FROM caja</sql>
 </querysource>
 <querysource>
  <name>caja_total</name>
  <sql>SELECT SUM( saldo ) AS total
FROM caja</sql>
 </querysource>
 <querysource>
  <name>ctacte_item</name>
  <sql>SELECT cc.numero_cuenta AS num,
             c.razon_social AS nombre,
             cc.saldo AS saldo
FROM ctacte AS cc, clientes AS c
WHERE cc.id_cliente = c.id
             AND cc.saldo > 0;</sql>
 </querysource>
 <querysource>
  <name>ctacte_total</name>
  <sql>SELECT SUM( saldo ) AS total
FROM ctacte</sql>
 </querysource>
