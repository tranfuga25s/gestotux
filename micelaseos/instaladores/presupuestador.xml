                        </distributionFile>
                        <distributionFile>
                            <origin>../bin/QtNetwork4.dll</origin>
                        </distributionFile>
                        <distributionFile>
                            <origin>../bin/QtSql4.dll</origin>
                        </distributionFile>
                        <distributionFile>
